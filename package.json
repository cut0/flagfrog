{
  "name": "flagfrog",
  "version": "0.1.9",
  "description": "A tool to manage flags within an application",
  "repository": {
    "type": "git",
    "url": "https://github.com/cut0/flagfrog.git"
  },
  "main": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./*": "./*"
  },
  "bin": {
    "flagfrog": "./dist/cli/index.js"
  },
  "scripts": {
    "lint:check": "biome ci",
    "lint:fix": "biome check --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p ./tsconfig.build.json && tsx ./build.ts",
    "cli:dev": "tsx ./src/cli/main.ts",
    "cli:prd": "node ./dist/cli/index.js",
    "test": "node --import tsx --test **/*.test.ts",
    "release": "pnpm build && changeset publish"
  },
  "keywords": [
    "flag",
    "feature",
    "management",
    "cli",
    "tool"
  ],
  "author": "Cut0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "commander": "13.1.0",
    "inquirer": "12.4.2",
    "ts-morph": "25.0.1",
    "glob": "11.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.12",
    "@types/node": "22.13.2",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "esbuild": "0.25.0",
    "tsx": "4.19.2",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "packageManager": "pnpm@10.6.2+sha512.47870716bea1572b53df34ad8647b42962bc790ce2bf4562ba0f643237d7302a3d6a8ecef9e4bdfc01d23af1969aa90485d4cebb0b9638fa5ef1daef656f6c1b"
}
