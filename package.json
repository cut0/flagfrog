{
  "name": "flagfrog",
  "version": "0.0.3",
  "description": "A tool to manage flags within an application",
  "repository": {
    "type": "git",
    "url": "https://github.com/cut0/flagfrog.git"
  },
  "main": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./*": "./*"
  },
  "bin": {
    "flagfrog": "./dist/cli/index.js"
  },
  "scripts": {
    "lint:check": "biome ci",
    "lint:fix": "biome check --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p ./tsconfig.build.json && tsx ./build.ts",
    "cli:dev": "tsx ./src/cli/main.ts",
    "cli:prd": "node ./dist/cli/index.js",
    "test": "node --import tsx --test **/*.test.ts",
    "release": "pnpm build && changeset publish"
  },
  "keywords": [
    "flag",
    "feature",
    "management",
    "cli",
    "tool"
  ],
  "author": "Cut0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "commander": "13.0.0",
    "inquirer": "12.0.1",
    "ts-morph": "24.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.9",
    "@types/node": "22.9.0",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "esbuild": "0.24.2",
    "tsx": "4.19.2",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "packageManager": "pnpm@8.15.9+sha512.499434c9d8fdd1a2794ebf4552b3b25c0a633abcee5bb15e7b5de90f32f47b513aca98cd5cfd001c31f0db454bc3804edccd578501e4ca293a6816166bbd9f81"
}
