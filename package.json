{
  "name": "flagfrog",
  "version": "0.1.10",
  "description": "A tool to manage flags within an application",
  "repository": {
    "type": "git",
    "url": "https://github.com/cut0/flagfrog.git"
  },
  "main": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./*": "./*"
  },
  "bin": {
    "flagfrog": "./dist/cli/index.js"
  },
  "scripts": {
    "lint:check": "biome ci",
    "lint:fix": "biome check --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p ./tsconfig.build.json && tsx ./build.ts",
    "cli:dev": "tsx ./src/cli/main.ts",
    "cli:prd": "node ./dist/cli/index.js",
    "test": "node --import tsx --test **/*.test.ts",
    "release": "pnpm build && changeset publish"
  },
  "keywords": [
    "flag",
    "feature",
    "management",
    "cli",
    "tool"
  ],
  "author": "Cut0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "commander": "13.1.0",
    "inquirer": "12.5.2",
    "ts-morph": "25.0.1",
    "glob": "11.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.28.1",
    "@types/node": "22.13.2",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "esbuild": "0.25.4",
    "tsx": "4.19.2",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
}
